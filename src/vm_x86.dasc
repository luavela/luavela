|// Compare hotcounter and trigger trace recorder
|.macro hotloop, offset
|  mov XCHGw, [PC + offset]
|  cmp XCHGw, 1
|  jb ->vm_hotloop
|.endmacro
|
|.macro hotcall, offset
|  mov XCHGw, [PC + offset]
|  cmp XCHGw, 0
|  jz ->vm_hotcall
|.endmacro
