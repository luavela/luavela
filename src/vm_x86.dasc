|//-----------------------------------------------------------------------
|// Calculate number of stack slots based on two stack pointers. The resulting
|// value is incremented by two in order to comply to NARGS format.
|.macro ptrdiff2nargs, acc, top, base
|  mov acc, top
|  sub acc, base
|  shr acc, TVB
|  add NARGS:acc, 1
|.endmacro
